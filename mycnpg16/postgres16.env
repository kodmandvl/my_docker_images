export PGHOME=/usr/lib/postgresql/16
export MANPATH=/usr/share/postgresql/16/man:$MANPATH
export LD_LIBRARY_PATH=$PGHOME/lib:${LD_LIBRARY_PATH}
export PATH=$PGHOME/bin:$PATH
export PG_PLUGINS_PATH=$PGHOME/lib
#export PGDATA=/var/lib/postgresql/data
export TZ="Europe/Moscow"
export PS1="\[\e[33m\][\h:\W]$ \[\e[0m\]"
alias reload='$PGHOME/bin/pg_ctl -w reload'
alias status='$PGHOME/bin/pg_ctl -w status'
